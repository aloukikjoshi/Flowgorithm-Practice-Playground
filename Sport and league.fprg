                                    <assign variable="z" expression="42"/>
                                    <output expression="&quot;Real sociedad &quot;&amp;z" newline="True"/>
                                    <if expression="x&gt;y &amp;&amp; x&gt;z">
                                        <then>
                                            <output expression="&quot;table topper is barcelona&quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <if expression="y&gt;z &amp;&amp; y&gt;x">
                                                <then>
                                                    <output expression="&quot;table topper is real madrid&quot;" newline="True"/>
                                                </then>
                                                <else>
                                                    <output expression="&quot;table topper is real sociedad&quot;" newline="True"/>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                    <output expression="&quot;top goaler&quot;" newline="True"/>
                                    <assign variable="xy" expression="13"/>
                                    <output expression="&quot;R. lewandowski &quot;&amp;xy" newline="True"/>
                                    <assign variable="yz" expression="11"/>
                                    <output expression="&quot;joselu &quot;&amp;yz" newline="True"/>
                                    <assign variable="zx" expression="9"/>
                                    <output expression="&quot;k. benzema &quot;&amp;zx" newline="True"/>
                                    <if expression="xy&gt;yz &amp;&amp; xy&gt;zx">
                                        <then>
                                            <output expression="&quot;r. lewandoski is leading goal scorer&quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <if expression="yz&gt;xy and yz&gt;zx">
                                                <then>
                                                    <output expression="&quot;joselu is leading goal scorer&quot;" newline="True"/>
                                                </then>
                                                <else>
                                                    <output expression="&quot;k. benzema is leading goal scorer&quot;" newline="True"/>
                                                </else>
                                            </if>
                                        </else>
                                    </if>
                                </then>
                                <else>
                                    <if expression="i==3">
                                        <then>
                                            <output expression="&quot;PPG table&quot;" newline="True"/>
                                            <assign variable="x" expression="117.6"/>
                                            <output expression="&quot;boston &quot;&amp;x" newline="True"/>
                                            <assign variable="y" expression="114.6"/>
                                            <output expression="&quot;milwaukee &quot;&amp;y" newline="True"/>
                                            <assign variable="z" expression="114.3"/>
                                            <output expression="&quot;philadelphia &quot;&amp;z" newline="True"/>
                                            <if expression="x&gt;y &amp;&amp; x&gt;z">
                                                <then>
                                                    <output expression="&quot;table topper is boston&quot;" newline="True"/>
                                                </then>
                                                <else>
                                                    <if expression="y&gt;z &amp;&amp; y&gt;x">
                                                        <then>
                                                            <output expression="&quot;table topper is milwaukee&quot;" newline="True"/>
                                                        </then>
                                                        <else>
                                                            <output expression="&quot;table topper is philadelphia&quot;" newline="True"/>
                                                        </else>
                                                    </if>
                                                </else>
                                            </if>
                                            <output expression="&quot;top PPG&quot;" newline="True"/>
                                            <assign variable="xy" expression="33.2"/>
                                            <output expression="&quot;l. doncic &quot;&amp;xy" newline="True"/>
                                            <assign variable="yz" expression="33.1"/>
                                            <output expression="&quot;j. embiid &quot;&amp;yz" newline="True"/>
                                            <assign variable="zx" expression="32.4"/>
                                            <output expression="&quot;G. Antetokounmpo &quot;&amp;zx" newline="True"/>
                                            <if expression="xy&gt;yz &amp;&amp; xy&gt;zx">
                                                <then>
                                                    <output expression="&quot;l. doncic is leading pointer&quot;" newline="True"/>
                                                </then>
                                                <else>
                                                    <if expression="yz&gt;xy &amp;&amp; yz&gt;zx">
                                                        <then>
                                                            <output expression="&quot;j. embiid is pointer&quot;" newline="True"/>
                                                        </then>
                                                        <else>
                                                            <output expression="&quot;G. Antetokounmpo is pointer&quot;" newline="True"/>
                                                        </else>
                                                    </if>
                                                </else>
                                            </if>
                                        </then>
                                        <else/>
                                    </if>
                                </else>
                            </if>
                        </else>
                    </if>
                </then>
                <else>
                    <output expression="&quot;invalid match&quot;" newline="True"/>
                </else>
            </if>
        </body>
