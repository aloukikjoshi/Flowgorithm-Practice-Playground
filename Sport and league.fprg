                            <output expression="&quot;MI &quot;&amp;x" newline="True"/>
                            <assign variable="y" expression="14"/>
                            <output expression="&quot;CSK &quot;&amp;y" newline="True"/>
                            <assign variable="z" expression="18"/>
                            <output expression="&quot;GT &quot;&amp;z" newline="True"/>
                            <if expression="x&gt;y &amp;&amp; x&gt;z">
                                <then>
                                    <output expression="&quot;table topper is MI&quot;" newline="True"/>
                                </then>
                                <else>
                                    <if expression="y&gt;z &amp;&amp; y&gt;x">
                                        <then>
                                            <output expression="&quot;table topper is CSK&quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <output expression="&quot;table topper is GT&quot;" newline="True"/>
                                        </else>
                                    </if>
                                </else>
                            </if>
                            <output expression="&quot;top scorer&quot;" newline="True"/>
                            <assign variable="xy" expression="632"/>
                            <output expression="&quot;virat kohli &quot;&amp;xy" newline="True"/>
                            <assign variable="yz" expression="530"/>
                            <output expression="&quot;david warner &quot;&amp;yz" newline="True"/>
                            <assign variable="zx" expression="492"/>
                            <output expression="&quot;rohit sharma &quot;&amp;zx" newline="True"/>
                            <if expression="xy&gt;yz &amp;&amp; xy&gt;zx">
                                <then>
                                    <output expression="&quot;virat kohli is leading run scorer&quot;" newline="True"/>
                                </then>
                                <else>
                                    <if expression="yz&gt;xy and yz&gt;zx">
                                        <then>
                                            <output expression="&quot;david warner is leading run scorer&quot;" newline="True"/>
                                        </then>
                                        <else>
                                            <output expression="&quot;rohit sharma is leading run scorer&quot;" newline="True"/>
                                        </else>
                                    </if>
                                </else>
                            </if>
                        </then>
                        <else>
                            <if expression="i==2">
                                <then>
                                    <output expression="&quot;point table&quot;" newline="True"/>
                                    <assign variable="x" expression="56"/>
