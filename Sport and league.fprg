    <function name="Main" type="None" variable="">
        <parameters/>
        <body>
            <declare name="x, y, z, xy, yz, zx" type="Real" array="False" size=""/>
            <declare name="a" type="String" array="True" size="3"/>
            <declare name="i" type="Integer" array="False" size=""/>
            <assign variable="i" expression="0"/>
            <assign variable="a[i]" expression="&quot;cricket&quot;"/>
            <assign variable="i" expression="1"/>
            <assign variable="a[i]" expression="&quot;football&quot;"/>
            <assign variable="i" expression="2"/>
            <assign variable="a[i]" expression="&quot;basketball&quot;"/>
            <output expression="&quot;select sport: 1-cricket, 2-football, 3-basketball&quot;" newline="True"/>
            <input variable="i"/>
            <output expression="a[i-1]" newline="True"/>
            <declare name="b" type="String" array="True" size="3"/>
            <declare name="j" type="Integer" array="False" size=""/>
            <assign variable="j" expression="0"/>
            <assign variable="b[j]" expression="&quot;IPL&quot;"/>
            <assign variable="j" expression="1"/>
            <assign variable="b[j]" expression="&quot;La Liga&quot;"/>
            <assign variable="j" expression="2"/>
            <assign variable="b[j]" expression="&quot;NBL&quot;"/>
            <output expression="&quot;select league: 1-IPL, 2-La Liga, 3-NBL&quot;" newline="True"/>
            <input variable="j"/>
            <output expression="b[j-1]" newline="True"/>
            <if expression="i=j">
                <then>
                    <if expression="i==1">
                        <then>
                            <output expression="&quot;point table&quot;" newline="True"/>
